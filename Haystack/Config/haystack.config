bind 192.168.3.22 BSDServer
bind 192.168.3.23 BSDServer
bind 192.168.3.24 WinServer
bind 192.168.3.25 WinServer
bind 192.168.3.26 BSDServer
bind 192.168.3.27 LinuxServer
bind 192.168.3.28 BSDServer
bind 192.168.3.29 WinServer
bind 192.168.3.30 LinuxServer
bind 192.168.3.31 LinuxServer
