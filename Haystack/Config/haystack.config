create default
set default default tcp action block
set default default udp action block
set default default icmp action block

create WinServer
set WinServer personality "Microsoft Windows 2003 Server"
set WinServer default tcp action reset
set WinServer ethernet "Dell"
add WinServer tcp port 20 open
add WinServer tcp port 21 "/usr/share/honeyd/scripts/win32/win2k/msftp.sh"
add WinServer tcp port 23 "/usr/share/honeyd/scripts/router-telnet.pl"
add WinServer tcp port 80 "/usr/share/honeyd/scripts/win32/web.sh"
add WinServer tcp port 139 open
add WinServer tcp port 137 open
add WinServer udp port 137 open
add WinServer udp port 135 open 
set WinServer uptime 1546550 

create LinuxServer
set LinuxServer personality "Linux kernel 2.6.5 - 2.6.8"
set LinuxServer default tcp action reset
set LinuxServer ethernet "Dell"
add LinuxServer tcp port 80 "/usr/share/honeyd/scripts/web.sh"
add LinuxServer tcp port 139 open
add LinuxServer tcp port 137 open
add LinuxServer udp port 137 open
add LinuxServer udp port 135 open 
set LinuxServer uptime 1324350 

create BSDServer
set BSDServer personality "Linux kernel 2.6.5 - 2.6.8"
set BSDServer default tcp action reset
set BSDServer ethernet "Dell"
add BSDServer tcp port 23 "/usr/share/honeyd/scripts/router-telnet.pl"
add BSDServer tcp port 139 open
add BSDServer tcp port 137 open
add BSDServer udp port 137 open
add BSDServer udp port 135 open 
set BSDServer uptime 1326346 

bind 192.168.3.10 WinServer
bind 192.168.3.11 BSDServer
bind 192.168.3.12 LinuxServer
bind 192.168.3.13 BSDServer
bind 192.168.3.14 BSDServer
bind 192.168.3.15 WinServer
bind 192.168.3.16 LinuxServer
bind 192.168.3.17 LinuxServer
bind 192.168.3.18 WinServer
bind 192.168.3.19 LinuxServer
bind 192.168.3.20 BSDServer
